public class Friend {
    private String name;
    private int age;
    private char gender;

    Friend(String name) {
        this.name = name;
    }
    Friend (String name, int age) {
        this.name = name;
        this.age = age;        
    }
    Friend (String name, int age, char gender) {
        this.name = name;
        this.age = age;
        this.gender = gender;
        
    }        
}
